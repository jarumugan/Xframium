	<function name="fn_MoveTo_SpecificField_EnterAddress">
		<description> fn_MoveTo_SpecificField_EnterAddress - Function to move to specific field on Enter Address page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="DEVICE" name="HIDE_KEYBOARD" site="FordPass" page="pge_CommonReusable" os="ANDROID" failureMode="IGNORE" />
		
		<step type="EXISTS" name="edt_EnterAddressField" page="pge_EnterAddress" failureMode="IGNORE" os="ANDROID">
			<token type="CONTEXT" name="token_edt_EnterAddressField_AND" value="param_AndroidToken"/>
			<token type="CONTEXT" name="token_edt_EnterAddressField_IOS" value="param_iOSToken"/>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_AndroidToken" />
				<parameter type="STATIC" value="Above field is available on Enter Address page along with its expected label and Textfield" />
			</step>
			
			<step type="STATE" name="Dummy State - Object appears" page="pge_EnterAddress" failureMode="IGNORE" />
			
			<step type="BREAK" name="Object appears successfully" page="pge_EnterAddress" />
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="CALL2" name="fn_SwipeToBottom_Parameterized" page="pge_CommonReusable">
					<parameter type="STATIC" name="param_NoOfSwipe" value="1" />
				</step>
			</step>
		</step>
		
		<step type="WAIT_FOR" name="edt_EnterAddressField.VISIBLE" page="pge_EnterAddress" failureMode="IGNORE" os="IOS">
			<token type="CONTEXT" name="token_edt_EnterAddressField_AND" value="param_AndroidToken"/>
			<token type="CONTEXT" name="token_edt_EnterAddressField_IOS" value="param_iOSToken"/>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_AndroidToken" />
				<parameter type="STATIC" value="Above field is available on Enter Address page along with its expected label and Textfield" />
			</step>
			
			<step type="STATE" name="Dummy State - Object appears" page="pge_EnterAddress" failureMode="IGNORE" />
			
			<step type="BREAK" name="Object appears successfully" page="pge_EnterAddress" />
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="CALL2" name="fn_SwipeToBottom_Parameterized" page="pge_CommonReusable">
					<parameter type="STATIC" name="param_NoOfSwipe" value="1" />
				</step>
			</step>
		</step>
	</function>

	<function name="fn_ValidateEnterAddress_NextButtonStatus">
		<description> fn_ValidateEnterAddress_NextButtonStatus - Function to validate/check the status of Next button on 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" os="ANDROID" />		
		
		<step type="VISIBLE" name="btn_Next" page="pge_EnterAddress" failureMode="IGNORE" >
			<parameter type="STATIC" name="maxScroll" value="6" />
			<parameter type="STATIC" name="direction" value="UP" />
			<token type="CONTENT" name="token_btn_Next_AND" value="TEnterAddress12_AND" />
			<token type="CONTENT" name="token_btn_Next_IOS" value="TEnterAddress12_IOS" />
		</step>
		
		<step type="EXISTS" name="btn_Next" page="pge_EnterAddress">
			<token type="CONTENT" name="token_btn_Next_AND" value="TEnterAddress12_AND" />
			<token type="CONTENT" name="token_btn_Next_IOS" value="TEnterAddress12_IOS" />
			
			<step type="ATTRIBUTE" name="btn_Next" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_ExpectedStatus" />
				<parameter type="STATIC" value="enabled" />
				<token type="CONTENT" name="token_btn_Next_AND" value="TEnterAddress12_AND" />
				<token type="CONTENT" name="token_btn_Next_IOS" value="TEnterAddress12_IOS" />
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTEXT" value="param_ExpectedStatus" />
					<parameter type="STATIC" value="'Next' button's enabled property value as expected on 'Enter Address' page" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
						<parameter type="CONTEXT" value="param_ExpectedStatus" />
						<parameter type="STATIC" value="'Next' button's enabled property value not as expected on 'Enter Address' page" />
					</step>
				</step>
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Next' button is not available on 'Enter Address' page" />
				</step>
			</step>
		</step>
	</function>

	<function name="fn_ClickEnterAddress_Next">
		<description> fn_ClickEnterAddress_Next - Function to click Next button on 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="CALL2" name="fn_ValidateEnterAddress_NextButtonStatus" page="pge_EnterAddress">
			<parameter type="STATIC" name="param_ExpectedStatus" value="true" />
		</step>
		
		<step type="EXISTS" name="btn_Next" page="pge_EnterAddress">
			<token type="CONTENT" name="token_btn_Next_AND" value="TEnterAddress12_AND" />
			<token type="CONTENT" name="token_btn_Next_IOS" value="TEnterAddress12_IOS" />
			
			<step type="CLICK" name="btn_Next" page="pge_EnterAddress" wait="3">
				<token type="CONTENT" name="token_btn_Next_AND" value="TEnterAddress12_AND" />
				<token type="CONTENT" name="token_btn_Next_IOS" value="TEnterAddress12_IOS" />
			</step>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="STATIC" value="'Next' button is available and clicked on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Next' button is not available on 'Enter Address' page" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_CheckforInlineError_EnterAddress">
		<description> fn_CheckforInlineError_EnterAddress - Function to validate for Inline error message displayed next to field in Enter Address page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
				
		<!-- To Hide the Keyboard -->
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" os="ANDROID"/>
		
		<step type="STATE" name="Dummy State - Check for Inline error" page="pge_EnterAddress" failureMode="IGNORE" />
		
		<step type="EXISTS" name="lbl_InlineErrorMsg" page="pge_EnterAddress">
			<token type="CONTEXT" name="token_lbl_InlineErrorMsg" value="param_InlineErrorMessage" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_InlineErrorMessage" />
				<parameter type="STATIC" value="Above inline error message displayed bottom of the field as expected" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTEXT" value="param_InlineErrorMessage" />
					<parameter type="STATIC" value="Above inline error message displayed botton of the field as expected" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_CheckNotExist_InlineError_EnterAddress">
		<description> fn_CheckNotExist_InlineError_EnterAddress - Function to validate for Inline error message is not displayed next to field in Enter Address page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
				
		<!-- To Hide the Keyboard -->
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" os="ANDROID"/>
		
		<step type="STATE" name="Dummy State - Check for Inline error not exists" page="pge_EnterAddress" failureMode="IGNORE" />
		
		<step type="EXISTS" name="lbl_InlineErrorMsg" page="pge_EnterAddress" failureMode="IGNORE">
			<token type="CONTEXT" name="token_lbl_InlineErrorMsg" value="param_InlineErrorMessage" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_InlineErrorMessage" />
				<parameter type="STATIC" value="Above inline error message is displayed bottom to the field but it should not" />
			</step>
			
			<!-- stop the execution -->
			<step type="FLOW" name="END_FAILURE" page="pge_EnterAddress" failureMode="ERROR"/>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTEXT" value="param_InlineErrorMessage" />
					<parameter type="STATIC" value="Above inline error message displayed bottom to the above field as expected" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_ValidateFieldsWithExpectedValue_EnterAddress">
		<description> fn_ValidateFieldsWithExpectedValue_EnterAddress - Function to validate whether expected values are pre-filled on the Enter Address page fields </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="LOOP" name="WaitTillObjectAppears" page="pge_EnterAddress" failureMode="IGNORE">
			<parameter type="STATIC" value="3" />
			<parameter type="STATIC" value="fn_MoveTo_SpecificField_EnterAddress" />
		</step>
		
		<step type="EXISTS" name="edt_EnterAddressField" page="pge_EnterAddress">
			<token type="CONTEXT" name="token_edt_EnterAddressField_AND" value="param_AndroidToken"/>
			<token type="CONTEXT" name="token_edt_EnterAddressField_IOS" value="param_iOSToken"/>
			
			<step type="ATTRIBUTE" name="edt_EnterAddressField" page="pge_EnterAddress">
				<parameter type="CONTEXT" value="param_ExpectedValue" />
				<parameter type="STATIC" value="text" />
				<token type="CONTEXT" name="token_edt_EnterAddressField_AND" value="param_AndroidToken"/>
				<token type="CONTEXT" name="token_edt_EnterAddressField_IOS" value="param_iOSToken"/>
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTEXT" value="param_ExpectedValue" />
					<parameter type="STATIC" value="Above expected value is available on the below field" />
					<parameter type="CONTEXT" value="param_AndroidToken" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
						<parameter type="CONTEXT" value="param_ExpectedValue" />
						<parameter type="STATIC" value="Above expected value is not available on the below field" />
						<parameter type="CONTEXT" value="param_AndroidToken" />
					</step>
				</step>
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTEXT" value="param_AndroidToken" />
					<parameter type="STATIC" value="Above mentioned field is not available on the Enter Address field" />
				</step>
			</step>
		</step>
