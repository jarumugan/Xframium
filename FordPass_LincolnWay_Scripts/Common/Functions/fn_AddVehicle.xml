              <description> fn_Click_Close_Smartlink_Information_Dialog - Function to click close button of Access FordPass Smartlink Info Dialog</description>
              <step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
                     <parameter type="STATIC" value="Function's XML file name: pge_AddVIN" />
              </step>
     
     
	<step type="CLICK" name="btn_Close_Smartlink_Info_Dialog" page="pge_AddVIN">
	<token type="CONTENT" name="token_btn_Close_Smartlink_Info_Dialog" value="TSmartlink10" />
	  <step type="WAIT_FOR" name="lbl_AccessFordPass_Smartlink" page="pge_AddVIN">
	  <parameter type="STATIC" value="30"/>
	  <token type="CONTENT" name="token_lbl_AccessFordPass_Smartlink" value="TSmartlink1" />
	  </step>
	  
	  			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="'ACCESS FORDPASS SMARTLINK' Info screen is displayed" />
		</step>
		
		<step type="ELSE" name="elseCondition" page="pge_AddVIN">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="'ACCESS FORDPASS SMARTLINK' Info screen is not displayed" />
			</step>
		</step>		
	</step>
	
	
</function>


