	<function name="fn_ValidateEyeIcon_NewPassword">
		<description> fn_ValidateEyeIcon_NewPassword - Function to validate New Password field's Eye Icon (masking the values) on 'Reset Password' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
			<parameter type="STATIC" value="Function's XML file name: fn_ResetPassword.xml" />
		</step>
		
		<step type="CALL2" name="fn_SwipeToBottom_Parameterized" page="pge_CommonReusable" failureMode="IGNORE">
			<parameter type="STATIC" name="param_NoOfSwipe" value="1" />
		</step>
		
		<!-- Input the New Password field with 'testfp' -->
		<step type="EXISTS" name="edt_NewPassword" page="pge_ResetPassword">
			<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
		
			<step type="SET" name="edt_NewPassword" page="pge_ResetPassword" wait="2">
				<parameter type="STATIC" name="value to set" value="testfp" />
				<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="STATIC" value="'New Password' field inputted with the 'testfp" />
				</step>
			</step>
		</step>
		
		<!-- By default New Password field values should be masked -->
		<step type="GET" name="edt_NewPassword" page="pge_ResetPassword" context="str_NewPasswordValue">
			<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
			
			<step type="COMPARE2" name="STRING" page="pge_ResetPassword">
				<parameter type="CONTEXT" name="Value One" value="str_NewPasswordValue" />
				<parameter type="STATIC" name="Value Two" value="••••••" />
				<parameter type="STATIC" name="Operator" value="EQUALS" />
				
				<step type="STATE" name="Screenshot - Password field value is masked by default" page="pge_ResetPassword" failureMode="IGNORE"/>
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="STATIC" value="By default - 'New Password' field value is displayed as masked" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_ResetPassword">
					<step type="STATE" name="Screenshot - New Password field value is not masked by default" page="pge_ResetPassword" />
					
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
						<parameter type="STATIC" value="By default - 'New Password' field value is not masked" />
					</step>
				</step>
			</step>
		</step>
		
		<!-- clicking on the Password Mask Eye image -->
		<step type="EXISTS" name="img_NewPasswordMask" page="pge_ResetPassword">
			<step type="CLICK" name="img_NewPasswordMask" page="pge_ResetPassword" wait="5" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
				<parameter type="STATIC" value="'Eye icon' on New Password field is available and clicked on 'Reset Password' page" />
			</step>
		</step>
		
		<!-- Check whether New Password field value is unmasked -->
		<step type="GET" name="edt_NewPwdWithoutMask" page="pge_ResetPassword" context="str_NewPasswordValue">
			<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
				
			<step type="COMPARE2" name="STRING" page="pge_ResetPassword">
				<parameter type="CONTEXT" name="Value One" value="str_NewPasswordValue" />
				<parameter type="STATIC" name="Value Two" value="testfp" />
				<parameter type="STATIC" name="Operator" value="EQUALS" />
				
				<step type="STATE" name="Screenshot - New Password field value is unmasked" page="pge_ResetPassword" failureMode="IGNORE"/>
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="STATIC" value="'New Password' field value is unmasked as expected" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_ResetPassword">
					<step type="STATE" name="Screenshot - New Password field value is not unmasked" page="pge_ResetPassword" />
					
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
						<parameter type="STATIC" value="'New Password' field value is not unmasked" />
					</step>
				</step>
			</step>
		</step>
		
		<!-- clicking on the Password Mask Eye image -->
		<step type="EXISTS" name="img_NewPasswordMask" page="pge_ResetPassword">
			<step type="CLICK" name="img_NewPasswordMask" page="pge_ResetPassword" wait="5" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
				<parameter type="STATIC" value="'Eye icon' on New Password field is available and clicked on 'Reset Password' page" />
			</step>
		</step>
		
		<!-- Check whether New Password field value masked once again -->
		<step type="GET" name="edt_NewPassword" page="pge_ResetPassword" context="str_NewPasswordValue">
			<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
			
			<step type="COMPARE2" name="STRING" page="pge_ResetPassword">
				<parameter type="CONTEXT" name="Value One" value="str_NewPasswordValue" />
				<parameter type="STATIC" name="Value Two" value="••••••" />
				<parameter type="STATIC" name="Operator" value="EQUALS" />
				
				<step type="STATE" name="Screenshot - New Password field value is masked" page="pge_ResetPassword" failureMode="IGNORE"/>
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="STATIC" value="'New Password' field value is masked again as expected after clicking Eye icon" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_ResetPassword">
					<step type="STATE" name="Screenshot - New Password field value is not masked" page="pge_ResetPassword" failureMode="IGNORE"/>
					
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
						<parameter type="STATIC" value="'New Password' field value is not masked as expected" />
					</step>
				</step>
			</step>
		</step>
		
		<step type="SET" name="edt_NewPassword" page="pge_ResetPassword" wait="2">
			<parameter type="STATIC" name="value to set" value="" />
			<token type="CONTENT" name="token_edt_NewPassword" value="TResetPassword5"/>
		</step>
		
		<!-- To Hide the Keyboard -->
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" />
	</function>

