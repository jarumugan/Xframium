	<function name="fn_EnterAddr_State">
            <description> ffn_EnterAddr_State - Function to validate and set the value to State dropdown field on Enter Address page </description>
            <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                  <parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
            </step>
            
<!--        <step type="COMPARE2" name="STRING" page="pge_EnterAddress" failureMode="IGNORE"> -->
<!--              <parameter type="CONTEXT" name="Value One" value="param_DataValue" /> -->
<!--              <parameter type="STATIC" name="Value Two" value="" /> -->
<!--              <parameter type="STATIC" name="Operator" value="EQUALS"/> -->
            
<!--              <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress"> -->
<!--                    <parameter type="STATIC" value="No changed made to the 'State' field on Edit Profile page" /> -->
<!--              </step> -->
                  
<!--              <step type="ELSE" name="elseCondition" page="pge_EnterAddress"> -->
                        <step type="EXISTS" name="edt_State" page="pge_EnterAddress">
                              <token type="CONTENT" name="token_edt_State" value="TEnterAddress9" />
                              
                              <step type="CLICK" name="edt_State" page="pge_EnterAddress" wait="5">
                                    <token type="CONTENT" name="token_edt_State" value="TEnterAddress9" />
                              </step>
                              
                              <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                    <parameter type="STATIC" value="'State' field is available and clicked on Enter Address page" />
                              </step>
                              
                              <step type="ELSE" name="elseCondition" page="pge_EnterAddress">
                                    <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                          <parameter type="STATIC" value="'State' field is available and clicked on Enter Address page" />
                                    </step>
                                    
                                    <step type="FLOW" name="END_FAILURE" page="pge_EnterAddress" failureMode="ERROR" />
                              </step>
                        </step>
                        
                        <!-- To set the pickerwheel value by SET method in IOS -->
                        <step type="SET" name="lst_State" page="pge_EnterAddress" os="IOS" wait="2">
                              <parameter type="DATA" name="Value to Set" value="TestData.State" />
                              <token type="DATA" name="token_lst_State" value="TestData.State" />
                              
                              <step type="ATTRIBUTE" name="lst_State" page="pge_EnterAddress">
                                    <parameter type="DATA" value="TestData.State" />
                                    <parameter type="STATIC" value="value" />
                                    <token type="DATA" name="token_lst_State" value="TestData.State" />
                                    
                                    <step type="STATE" name="Dummy State - State validation" page="pge_EnterAddress" failureMode="IGNORE" /> 
                                    
                                    <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                          <parameter type="DATA" value="TestData.State" />
                                          <parameter type="STATIC" value="Above value is available and selected successfully in 'State' dropdown/listbox field on Enter Address page" />
                                    </step>
                                    
                                    <!-- to click on Next link on top of Pickerwheel (iOS)-->
                                    <step type="CLICK" name="lnk_Next_Pickerwheel" page="pge_EnterAddress"  wait="5"/>
                              </step>
                              
                              <step type="ELSE" name="elseCondition" page="pge_EnterAddress">
                                    <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                          <parameter type="DATA" value="TestData.State" />
                                          <parameter type="STATIC" value="Above value is not available on 'State' dropdown/listbox field on Enter Address page" />
                                    </step>
                                    
                                    <step type="FLOW" name="END_FAILURE" page="pge_EnterAddress" failureMode="ERROR" />
                              </step>
                        </step>
                              
                        <!-- To set the Listbox/Dropdown value by CLICK method in ANDROID -->
                        <step type="VISIBLE" name="lst_State" page="pge_EnterAddress" os="ANDROID" failureMode="IGNORE">
                              <parameter type="STATIC" name="maxScroll" value="6" />
                              <parameter type="STATIC" name="direction" value="UP" />
                              <token type="DATA" name="token_lst_State" value="TestData.State" />
                        </step>
                              
                        <step type="EXISTS" name="lst_State" page="pge_EnterAddress" os="ANDROID" >
                              <token type="DATA" name="token_lst_State" value="TestData.State" />
                        
                              <step type="STATE" name="Dummy State - State validation" page="pge_EnterAddress" failureMode="IGNORE" />
                              
                              <step type="CLICK" name="lst_State" page="pge_EnterAddress">
                                    <token type="DATA" name="token_lst_State" value="TestData.State" />
                              </step>
                              
                              <step type="STATE" name="Dummy State - State validation" page="pge_EnterAddress" failureMode="IGNORE" />
                              
                              <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                    <parameter type="DATA" value="TestData.State" />
                                    <parameter type="STATIC" value="Above value is available and selected in 'State' dropdown/listbox field on Enter Address page" />
                              </step>
                              
                              <step type="ELSE" name="elseCondition" page="pge_EnterAddress">
                                    <step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
                                          <parameter type="DATA" value="TestData.State" />
                                          <parameter type="STATIC" value="Above value is not available on 'State' dropdown/listbox field on Enter Address page" />
                                    </step>
                              </step>
                        </step>
      </function>

	
	
