			<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
				<parameter type="CONTENT" value="TResetPassword2" />
				<parameter type="STATIC" value="Above text is displayed as page info on 'Reset Password' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_ResetPassword">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="CONTENT" value="TResetPassword2" />
