	<function name="fn_Verify_Categories">
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>
		<step type="CALL2" name="fn_Verify_CategoriesExistence" page="pge_FindCategoryApplet" failureMode="IGNORE">
			<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">	
				<step type="CALL2" name="fn_SwipeR_Categories" page="pge_FindCategoryApplet"/>
				<step type="CALL2" name="fn_Verify_CategoriesExistence" page="pge_FindCategoryApplet" failureMode="IGNORE">
					<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">	
						<step type="CALL2" name="fn_SwipeR_Categories" page="pge_FindCategoryApplet"/>
						<step type="CALL2" name="fn_Verify_CategoriesExistence" page="pge_FindCategoryApplet" failureMode="IGNORE">
							<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">	
								<step type="CALL2" name="fn_SwipeL_Categories" page="pge_FindCategoryApplet"/>
								<step type="CALL2" name="fn_Verify_CategoriesExistence" page="pge_FindCategoryApplet" failureMode="IGNORE">
									<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">	
										<step type="CALL2" name="fn_SwipeL_Categories" page="pge_FindCategoryApplet"/>
										<step type="CALL2" name="fn_Verify_CategoriesExistence" page="pge_FindCategoryApplet">
										</step>
									</step>
								</step>
							</step>
						</step>
					</step>
				</step>
			</step>
		</step>		
	</function>
	
	<function name="fn_Verify_CategoriesExistence">
	<description> fn_Verify_Categories: To verify the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>		
		<step type="EXISTS" name="lbl_Categories" page="pge_FindCategoryApplet">
		<token type="CONTEXT" name="token_lbl_Categories_IOS" value="param_Categories_IOS" />
		<token type="CONTEXT" name="token_lbl_Categories_AND" value="param_Categories_AND" />
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
			<parameter type="STATIC" value="'CATEGORY' is EXISTS with the following Name" />
			<parameter type="CONTEXT" name="Category_IOS" value="param_Categories_IOS" />
			<parameter type="CONTEXT" name="Category_AND" value="param_Categories_AND" />
		</step>
		<step type="STATE" name="FIND - CATEGORIES" page="pge_FindCommon" failureMode="IGNORE" />
			<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">	
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
					<parameter type="STATIC" value="Following 'CATEGORY' is NOT EXISTS on the Category Tray" />
					<parameter type="CONTEXT" name="Category_IOS" value="param_Categories_IOS"/>
					<parameter type="CONTEXT" name="Category_AND" value="param_Categories_AND" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_Click_Categories">
	<description> fn_Click_Categories: To Click the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>
		<step type="CLICK" name="lbl_Categories" page="pge_FindCategoryApplet" wait="2">		
			<token type="CONTEXT" name="token_lbl_Categories_IOS" value="param_Categories_IOS" />
			<token type="CONTEXT" name="token_lbl_Categories_AND" value="param_Categories_AND" />
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="Following 'CATEGORY' is CLICKED on the Category Tray" />
				<parameter type="CONTEXT" name="token_lbl_Categories_IOS" value="param_Categories_IOS" />
				<parameter type="CONTEXT" name="token_lbl_Categories_AND" value="param_Categories_AND" />
			</step>
			<step type="STATE" name="FIND - CATEGORIES" page="pge_FindCommon" failureMode="IGNORE" />
		<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="Following 'CATEGORY' is NOT CLICKED on the Category Tray"  />
				<parameter type="CONTEXT" name="token_lbl_Categories_IOS" value="param_Categories_IOS" />	
				<parameter type="CONTEXT" name="token_lbl_Categories_AND" value="param_Categories_AND" />
			</step>
			<step type="STATE" name="FIND - CATEGORIES" page="pge_FindCommon" failureMode="IGNORE" />
		</step>
		</step>
	</function>
	
	<function name="fn_Verify_Categories_Paginator">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>		
		<step type="EXISTS" name="btn_Categories_Paginator" page="pge_FindCategoryApplet">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY PAGINATOR' is EXISTS on the Page" />				
			</step>				
			<step type="STATE" name="FIND - CATEGORY PAGINATOR" page="pge_FindCommon" failureMode="IGNORE" />
		<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY PAGINATOR' is NOT EXISTS on the Page" />
			</step>
		</step>
		</step>
	</function>
	<function name="fn_SwipeR_Categories">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>					
		<step name="SWIPE.tray_Categories" page="pge_FindCategoryApplet" type="GESTURE">
			<parameter type="STATIC" name="From Position" value="90,50"/>
			<parameter type="STATIC" name="To Position" value="10,50"/>
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY' is SWIPED towards Right direction" />
			</step>
		  <step type="STATE" name="Filters - Category Swipe" page="pge_FindCategoryApplet" failureMode="IGNORE" />
		  <step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY' is NOT SWIPED towards Right direction" />
			</step>
		</step>
		</step>
	</function>
	<function name="fn_SwipeL_Categories">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>					
		<step name="SWIPE.tray_Categories" page="pge_FindCategoryApplet" type="GESTURE">
			<parameter type="STATIC" name="From Position" value="10,50"/>
			<parameter type="STATIC" name="To Position" value="90,50"/>
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY' is SWIPED towards Left direction" />
			</step>
		  <step type="STATE" name="Filters - Category Swipe" page="pge_FindCategoryApplet" failureMode="IGNORE" />
		  <step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY SWIPE' is NOT SWIPED towards Left direction" />
			</step>
		</step>
		</step>
	</function>
	</suite>