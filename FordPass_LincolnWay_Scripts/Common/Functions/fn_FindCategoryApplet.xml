			<parameter type="CONTENT" name="param_Categories_AND" value="TFindCategory13_AND" />
		</step>	
		<step type="CALL2" name="fn_Verify_Categories" page="pge_FindCategoryApplet" tagNames="FP-EU,FP_CH">
			<parameter type="CONTENT" name="param_Categories_IOS" value="TFindCategory14_IOS" />
			<parameter type="CONTENT" name="param_Categories_AND" value="TFindCategory14_AND" />
		</step>

		<step type="CALL2" name="fn_Verify_Categories" page="pge_FindCategoryApplet" tagNames="FP_CH">
			<parameter type="CONTENT" name="param_Categories_IOS" value="TFindCategory15_IOS" />
