	<function name="fn_VerifyPageOpened_ResetPassword">
		<description> fn_VerifyPageOpened_ResetPassword - Function to verify user landed on the 'Reset Password' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
			<parameter type="STATIC" value="Function's XML file name: fn_ResetPassword.xml" />
		</step>
		
		<step type="WAIT_FOR" name="lbl_PageHeader" page="pge_ResetPassword">
			<parameter type="STATIC" value="30" />
			<token type="CONTENT" name="token_lbl_PageHeader" value="TResetPassword1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
				<parameter type="STATIC" value="User successfully landed on 'Reset Password' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_ResetPassword">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_ResetPassword">
					<parameter type="STATIC" value="'Reset Password' page is not displayed or opened" />
				</step>
			</step>
		</step>
		
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" failureMode="IGNORE"/>
	</function>
	
