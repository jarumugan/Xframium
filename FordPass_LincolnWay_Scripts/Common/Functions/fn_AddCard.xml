	<function name="fn_VerifyPageOpened_AddCard">
		<description>fn_VerifyPageOpened_AddCard - Function to verify user landed on the 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
			<parameter type="STATIC" value="Function's XML file name: fn_AddCard.xml" />
		</step>
		
		<step type="WAIT_FOR" name="lbl_PageTitle" page="pge_AddCard" >
			<parameter type="STATIC" value="30" />
			<token type="CONTENT" name="token_lbl_PageTitle" value="TAddCard1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
				<parameter type="STATIC" value="User successfully landed on 'Addcard Info' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddCard">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
					<parameter type="STATIC" value="'AddCard Info' page is not displayed or page header is not as expected" />
				</step>
			</step>
		</step>
	</function>
	
