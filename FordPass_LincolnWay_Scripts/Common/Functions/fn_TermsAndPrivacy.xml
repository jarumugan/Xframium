	<function name="fn_SwipeUp">
	
		<step type="GESTURE" name="SWIPE" page="pge_OnBoardScreen1" wait="10">
		  <parameter type="STATIC" value="UP" />
		  <step type="REPORT" name="ADD_TO_REPORT">
		  	<parameter type="STATIC" value="The action Swipe Right was performed"/>
		  </step>
		</step>
		
	</function>
	
	<function name="fn_ClickCreateAccAndSwipe">
	
		<step type="CALL2" name="fn_clickCreateAccount" page="pge_Login" />
		
		 <step type="WAIT" name="Hard_Wait" page="pge_Park">
			<parameter type="STATIC" value="10" />
		</step>	
			
		<step type="CALL2" name="fn_SwipeUp" page="pge_Login" />
	</function>


	<function name="fn_ClickCreateAccountNoSwipe">
	
