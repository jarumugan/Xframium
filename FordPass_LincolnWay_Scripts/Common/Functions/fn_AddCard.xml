	<function name="fn_MoveTo_SpecificField_AddCard">
		<description> fn_MoveTo_SpecificField_AddCard - Function to move to specific field on Enter Address page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
			<parameter type="STATIC" value="Function's XML file name: fn_AddCard.xml" />
		</step>
		
		<step type="DEVICE" name="HIDE_KEYBOARD" site="FordPass" page="pge_CommonReusable" os="ANDROID" failureMode="IGNORE" />
		
		<step type="EXISTS" name="edt_AddCardField" page="pge_AddCard" failureMode="IGNORE" os="ANDROID">
			<token type="CONTEXT" name="token_edt_AddCardField_AND" value="param_AndroidToken"/>
			<token type="CONTEXT" name="token_edt_AddCardField_IOS" value="param_iOSToken"/>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
				<parameter type="CONTEXT" value="param_AndroidToken" />
				<parameter type="STATIC" value="Above field is available on Enter Address page along with its expected label and Textfield" />
			</step>
			
			<step type="STATE" name="Dummy State - Object appears" page="pge_AddCard" failureMode="IGNORE" />
			
			<step type="BREAK" name="Object appears successfully" page="pge_AddCard" />
			
			<step type="ELSE" name="elseCondition" page="pge_AddCard">
				<step type="CALL2" name="fn_SwipeToBottom_Parameterized" page="pge_CommonReusable">
					<parameter type="STATIC" name="param_NoOfSwipe" value="1" />
				</step>
			</step>
		</step>
		
		<step type="WAIT_FOR" name="edt_AddCardField.VISIBLE" page="pge_AddCard" failureMode="IGNORE" os="IOS">
			<token type="CONTEXT" name="token_edt_AddCardField_AND" value="param_AndroidToken"/>
			<token type="CONTEXT" name="token_edt_AddCardField_IOS" value="param_iOSToken"/>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
				<parameter type="CONTEXT" value="param_AndroidToken" />
				<parameter type="STATIC" value="Above field is available on Enter Address page along with its expected label and Textfield" />
			</step>
			
			<step type="STATE" name="Dummy State - Object appears" page="pge_AddCard" failureMode="IGNORE" />
			
			<step type="BREAK" name="Object appears successfully" page="pge_AddCard" />
			
			<step type="ELSE" name="elseCondition" page="pge_AddCard">
				<step type="CALL2" name="fn_SwipeToBottom_Parameterized" page="pge_CommonReusable">
					<parameter type="STATIC" name="param_NoOfSwipe" value="1" />
				</step>
			</step>
		</step>
	</function>

	<function name="fn_ValidateAddCard_SaveButtonStatus">
		<description> fn_ValidateAddCard_SaveButtonStatus - Function to validate/check the status of Save button on 'AddCard' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
			<parameter type="STATIC" value="Function's XML file name: fn_AddCard.xml" />
		</step>
		
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" os="ANDROID" />		
		
		<step type="VISIBLE" name="btn_Save" page="pge_AddCard" failureMode="IGNORE" >
			<parameter type="STATIC" name="maxScroll" value="6" />
			<parameter type="STATIC" name="direction" value="UP" />
			<token type="CONTENT" name="token_btn_Save_AND" value="TAddCard19_AND" />
			<token type="CONTENT" name="token_btn_Save_IOS" value="TAddCard19_IOS" />
		</step>
			
			<step type="ATTRIBUTE" name="btn_Save" page="pge_AddCard">
				<parameter type="CONTEXT" value="param_ExpectedStatus" />
				<parameter type="STATIC" value="enabled" />
				<token type="CONTENT" name="token_btn_Save_AND" value="TAddCard19_AND" />
				<token type="CONTENT" name="token_btn_Save_IOS" value="TAddCard19_IOS" />
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
					<parameter type="CONTEXT" value="param_ExpectedStatus" />
					<parameter type="STATIC" value="'Save' button's enabled property value as expected on 'Enter Address' page" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_AddCard">
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
						<parameter type="CONTEXT" value="param_ExpectedStatus" />
						<parameter type="STATIC" value="'Save' button's enabled property value not as expected on 'Enter Address' page" />
					</step>
				</step>
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddCard">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
					<parameter type="STATIC" value="'Save' button is not available on 'Enter Address' page" />
				</step>
			</step>
	
	</function>

	<function name="fn_ClickAddCard_Next">
		<description> fn_ClickAddCard_Next - Function to click Next button on 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
			<parameter type="STATIC" value="Function's XML file name: fn_AddCard.xml" />
		</step>
		
		<step type="CALL2" name="fn_ValidateAddCard_NextButtonStatus" page="pge_AddCard">
			<parameter type="STATIC" name="param_ExpectedStatus" value="true" />
		</step>
		
		<step type="EXISTS" name="btn_Next" page="pge_AddCard">
			<token type="CONTENT" name="token_btn_Next_AND" value="TAddCard12_AND" />
			<token type="CONTENT" name="token_btn_Next_IOS" value="TAddCard12_IOS" />
			
			<step type="CLICK" name="btn_Next" page="pge_AddCard" wait="3">
				<token type="CONTENT" name="token_btn_Next_AND" value="TAddCard12_AND" />
				<token type="CONTENT" name="token_btn_Next_IOS" value="TAddCard12_IOS" />
			</step>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
				<parameter type="STATIC" value="'Next' button is available and clicked on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddCard">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
					<parameter type="STATIC" value="'Next' button is not available on 'Enter Address' page" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_CheckforInlineError_AddCard">
		<description> fn_CheckforInlineError_AddCard - Function to validate for Inline error message displayed next to field in Enter Address page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddCard">
			<parameter type="STATIC" value="Function's XML file name: fn_AddCard.xml" />
		</step>
				
