            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />
         </step>
	</function>
	
	
	<function name="fn_AddVehicle_ExistingVIN">
		<description> fn_Addvehicle_ExistingVIN - Function to validate error message for adding already existing VIN in user dashboard </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>
		
		<step type="SET" name="edt_VIN" page="pge_AddVIN" >
			 <parameter type="CONTEXT" name="VIN id" value="param_VIN" />			 
		</step>
		
			
		<step type="CLICK" name="lbl_AddVehicleHeader" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lbl_AddVehicleHeader" value="TAddVinHeader" />
		</step>
		
       	<step type="CLICK" name="btn_Next_Enabled" page="pge_AddVIN" >
        	<token type="CONTENT" name="token_btn_Next_Enabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_LowerCase" value="TCommon_KeyboardNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />                     
            <token type="CONTENT" name="token_btn_Next_Enabled_IOS" value="TSelectCountry2_IOS" />
        </step>
		
     <!--   	<step type="CLICK" name="btn_Next_Enabled" page="pge_AddVIN" >
        	<token type="CONTENT" name="token_btn_Next_Enabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_LowerCase" value="TCommon_KeyboardNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />                     
        </step> -->
		
		<step type="WAIT_FOR" name="lbl_AddVehicleError_Oops" page="pge_AddVIN" >
			<parameter type="STATIC" value="10" />
			<token type="CONTENT" name="token_lbl_AddVehicleError_Oops" value="TVehicleAlreadyExists1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on Vehicle already added error message page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddVIN">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
					<parameter type="STATIC" value="Vehicle already added error message page is not displayed or opened" />
				</step>
			</step>			
		</step>
		
		<step type="VISIBLE" name="img_AddVehicleError" page="pge_AddVIN" />
		
		<step type="VISIBLE" name="lbl_VehicleAlreadyExists" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lbl_VehicleAlreadyExists" value="TVehicleAlreadyExists2" />
		</step>
		
		<step type="VISIBLE" name="lbl_VIN" page="pge_AddVIN" >
			<token type="DATA" name="token_lbl_VIN" value="TestData.VIN" />
		</step>
		
		<step type="VISIBLE" name="btn_Back_VehicleAlreadyExists" page="pge_AddVIN" >
			<token type="CONTENT" name="token_btn_Back" value="TAddVinBack_AND" />
		</step>
			
		<step type="VISIBLE" name="btn_OK" page="pge_AddVIN" >
			<token type="CONTENT" name="token_btn_OK" value="TVehicleAlreadyExistsOK" />
		</step>
		
		<step type="CLICK" name="btn_OK" page="pge_AddVIN" >
			<token type="CONTENT" name="token_btn_OK" value="TVehicleAlreadyExistsOK" />
		
		</step>		
		
		<step type="WAIT_FOR" name="lbl_AddVehicleHeader" page="pge_AddVIN" >
				<parameter type="STATIC" value="10" />
			<token type="CONTENT" name="token_lbl_AddVehicleHeader" value="TAddVinHeader" />
			</step>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on Enter VIN page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddVIN">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
					<parameter type="STATIC" value="Enter VIN page is not displayed or opened" />
				</step>
			</step>			
		
	</function>
	
