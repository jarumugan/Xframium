							<parameter type="CONTEXT" name="value to set" value="param_DataValue" />
							<parameter type="STATIC" name="Clear the text box and enter" value="DEFAULT" />
							<token type="CONTEXT" name="token_edt_EditProfileField_AND" value="param_AndroidToken"/>
							<token type="CONTEXT" name="token_edt_EditProfileField_IOS" value="param_iOSToken"/>
						</step>
						
						<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
							<parameter type="CONTEXT" value="param_AndroidToken" />
							<parameter type="STATIC" value="Above Edit Profile page field is inputted with the below value" />
							<parameter type="CONTEXT" value="param_DataValue" />
						</step>
					</step>
				</step>
			</step>
		</step>		
	</function>
	
