	<function name="fn_Validate_CPF_EditProfile">
		<description> fn_Validate_CPF_EditProfile - Function to validate whether CPF field is Non-Editable in Edit Profile page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
			<parameter type="STATIC" value="Function's XML file name: fn_EditProfile.xml" />
		</step>
		
		<step type="EXISTS" name="edt_CPF" page="pge_EditProfile">
			<token type="CONTENT" name="token_edt_CPF" value="TEditProfile3" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
				<parameter type="STATIC" value="'CPF' label displayed on Edit Profile page as expected" />
			</step>
			
			<step type="GET" name="edt_CPF" page="pge_EditProfile" context="str_CPF_Value">
				<token type="CONTENT" name="token_edt_CPF" value="TEditProfile3" />
			
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="CONTEXT" value="str_CPF_Value" />
					<parameter type="STATIC" value="Above value is displayed for 'CPF' on Edit Profile page" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_EditProfile">
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
						<parameter type="STATIC" value="No value displayed for 'CPF' on Edit Profile page" />
					</step>
				</step>
			</step>
			
			<step type="ATTRIBUTE" name="edt_CPF" page="pge_EditProfile"  os="ANDROID" > 
				<parameter type="STATIC" value="false" />
				<parameter type="STATIC" value="enabled" />
				<token type="CONTENT" name="token_edt_CPF" value="TEditProfile3" />
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="STATIC" value="'CPF' field is in Non-Editable state as expected" />
				</step>
				
				<step type="ELSE" name="elseCondition" page="pge_EditProfile">
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
						<parameter type="STATIC" value="'CPF' field is not in Non-Editable state" />
					</step>
				</step>
			</step>
			
			<step type="ATTRIBUTE" name="edt_CPF" page="pge_EditProfile"  os="IOS" > 
				<parameter type="STATIC" value="true" />
				<parameter type="STATIC" value="enabled" />
				<token type="CONTENT" name="token_edt_CPF" value="TEditProfile3" />
				
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="STATIC" value="'CPF' field is in Non-Editable state as expected" />
				</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EditProfile">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="STATIC" value="'CPF' label and its value are not displayed on Edit Profile page" />
				</step>
			</step>
		</step>	
		</step>	
	</function>
	
	<function name="fn_ValidateInput_EP_Title">
		<description> fn_ValidateInput_EP_Title - Function to validate and set the value to TITLE (optional) dropdown field on Edit Profile page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
			<parameter type="STATIC" value="Function's XML file name: fn_EditProfile.xml" />
		</step>
		
		<step type="COMPARE2" name="STRING" page="pge_EditProfile" failureMode="IGNORE">
			<parameter type="CONTEXT" name="Value One" value="param_DataValue" />
			<parameter type="STATIC" name="Value Two" value="" />
			<parameter type="STATIC" name="Operator" value="EQUALS"/>
		
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
				<parameter type="STATIC" value="No changed made to the 'Title' field on Edit Profile page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EditProfile">
				<step type="EXISTS" name="edt_Title" page="pge_EditProfile">
					<token type="CONTENT" name="token_edt_Title" value="TEditProfile4" />
					
					<step type="CLICK" name="edt_Title" page="pge_EditProfile" wait="5">
						<token type="CONTENT" name="token_edt_Title" value="TEditProfile4" />
					</step>
					
					<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
						<parameter type="STATIC" value="'Title' field is available and clicked on Edit Profile page" />
					</step>
					
					<!-- To set the pickerwheel value by SET method in IOS -->
					<step type="SET" name="lst_Title" page="pge_EditProfile" os="IOS" wait="2">
						<parameter type="DATA" name="Value to Set" value="TestData.Title" />
						<token type="DATA" name="token_lst_Title" value="TestData.Title" />
						
						<step type="ATTRIBUTE" name="lst_Title" page="pge_EditProfile">
							<parameter type="DATA" value="TestData.Title" />
							<parameter type="STATIC" value="value" />
							<token type="DATA" name="token_lst_Title" value="TestData.Title" />
							
							<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
								<parameter type="DATA" value="TestData.Title" />
								<parameter type="STATIC" value="Above value is available and selected successfully in 'Title' dropdown/listbox field on Edit Profile page" />
							</step>
							
							<!-- to click on Next link on top of Pickerwheel (iOS)-->
							<step type="CLICK" name="lnk_Next_Pickerwheel" page="pge_EditProfile"  wait="5"/>
						</step>
						
						<step type="ELSE" name="elseCondition" page="pge_EditProfile">
							<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
								<parameter type="DATA" value="TestData.Title" />
								<parameter type="STATIC" value="Above value is not available on 'Title' dropdown/listbox field on Edit Profile page" />
							</step>
							
							<step type="FLOW" name="END_FAILURE" page="pge_EditProfile" failureMode="ERROR" />
						</step>
					</step>
					
					<!-- To set the Listbox/Dropdown value by CLICK method in ANDROID -->
					<step type="VISIBLE" name="lst_Title" page="pge_EditProfile" os="ANDROID">
						<parameter type="STATIC" name="maxScroll" value="3" />
						<parameter type="STATIC" name="direction" value="UP" />
						<token type="DATA" name="token_lst_Title" value="TestData.Title" />
						
						<step type="CLICK" name="lst_Title" page="pge_EditProfile">
							<token type="DATA" name="token_lst_Title" value="TestData.Title" />
						</step>
						
						<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
							<parameter type="DATA" value="TestData.Title" />
							<parameter type="STATIC" value="Above value is available and selected in 'Title' dropdown/listbox field on Edit Profile page" />
						</step>
						
						<step type="ELSE" name="elseCondition" page="pge_EditProfile">
							<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
								<parameter type="DATA" value="TestData.Title" />
								<parameter type="STATIC" value="Above value is not available on 'Title' dropdown/listbox field on Edit Profile page" />
							</step>
							
							<step type="FLOW" name="END_FAILURE" page="pge_EditProfile" failureMode="ERROR" />
						</step>
					</step>
										
					<step type="ELSE" name="elseCondition" page="pge_EditProfile">
						<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
							<parameter type="STATIC" value="'Title' field is available and clicked on Edit Profile page" />
						</step>
						
						<step type="FLOW" name="END_FAILURE" page="pge_EditProfile" failureMode="ERROR" />
					</step>
				</step>
			</step>
		</step>
	</function>
	
