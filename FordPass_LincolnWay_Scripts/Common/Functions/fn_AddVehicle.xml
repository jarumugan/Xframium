<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
	
	<function name="fn_ClickAddVehicle">
		<description> fn_ClickAddVehicle - Function to click Add Vehicle in Garage screen </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>
		
		<step type="CALL2" name="fn_Click_AddVehicleTab" page="pge_Move" />
		
		<step type="CLICK" name="lnk_AddVehicle" page="pge_AddVIN">
			<token type="CONTENT" name="token_lnk_AddVehicle" value="TAddVin" />			
		</step>
		
	</function>
	
	<function name="fn_VerifyPageOpened_AddVIN">
		<description> fn_VerifyPageOpened_AddVIN - Function to verify user landed on the ADD VIN page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>		
		
		<step type="WAIT_FOR" name="lbl_AddVehicleHeader" page="pge_AddVIN" >
			<parameter type="STATIC" value="10" />
			<token type="CONTENT" name="token_lbl_AddVehicleHeader" value="TAddVinHeader" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on Add Vehicle page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddVIN">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
					<parameter type="STATIC" value="Add Vehicle page is not displayed or opened" />
				</step>
			</step>
		</step>
	</function>
	
	
	<function name="fn_ValidatePage_AddVIN">
		<description> fn_VerifyPageOpened_AddVIN - Function to verify ADD VIN page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>
		
		<step type="EXISTS" name="lbl_AddVehicleHeader" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lbl_AddVehicleHeader" value="TAddVinHeader" />
		</step>
		
		<step type="EXISTS" name="lbl_AddVehicleDesc" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lbl_AddVehicleDesc" value="TAddVinDesc" />
		</step>
		
		<step type="EXISTS" name="edt_VIN" page="pge_AddVIN" />
					
		<step type="EXISTS" name="img_Camera" page="pge_AddVIN" />
		
		<step type="EXISTS" name="lnk_WhereIsMyVin" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lnk_WhereIsMyVin" value="TAddVinFind" />
		</step>
		
         <step type="EXISTS" name="btn_Next_Disabled" page="pge_AddVIN" >
            <token type="CONTENT" name="token_btn_Next_Disabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />
         </step>

		<step type="EXISTS" name="btn_Back" page="pge_AddVIN" >
			<token type="CONTENT" name="token_btn_Back" value="TAddVinBack_AND" />
		</step>
		
		<step type="CLICK" name="lnk_WhereIsMyVin" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lnk_WhereIsMyVin" value="TAddVinFind" />
		</step>
		
		<step type="WAIT_FOR" name="lbl_FindVinTitle" page="pge_AddVIN" >
			<parameter type="STATIC" value="10" />
			<token type="CONTENT" name="token_lbl_FindVinTitle" value="TAddVinFindTitle" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on Find VIN page" />
			</step>
			
			<step type="EXISTS" name="img_FindVinDoorFrame" page="pge_AddVIN">
				<token type="CONTENT" name="token_lbl_FindVinDoorFrame" value="TAddVinFindDoorFrame" />
			</step>
			
			<step type="EXISTS" name="lbl_FindVinDoorFrame" page="pge_AddVIN" >
				<token type="CONTENT" name="token_lbl_FindVinDoorFrame" value="TAddVinFindDoorFrame" />
			</step>
			
			<step type="EXISTS" name="img_FindVinDashboard" page="pge_AddVIN" tagNames="FP-EU,FP-NA">
				<token type="CONTENT" name="token_lbl_FindVinDashboard" value="TAddVinFindDashboard" />
			</step>
			
			<step type="EXISTS" name="lbl_FindVinDashboard" page="pge_AddVIN" tagNames="FP-EU,FP-NA">
				<token type="CONTENT" name="token_lbl_FindVinDashboard" value="TAddVinFindDashboard" />
			</step>
			
			<step type="EXISTS" name="img_FindVinDocument" page="pge_AddVIN">
				<token type="CONTENT" name="token_lbl_FindVinDocument" value="TAddVinFindDocument" />
			</step>
			
			<step type="EXISTS" name="lbl_FindVinDocument" page="pge_AddVIN" >
				<token type="CONTENT" name="token_lbl_FindVinDocument" value="TAddVinFindDocument" />
			</step>
			
			<step type="CLICK" name="btn_FindVinBack" page="pge_AddVIN" >
				<token type="CONTENT" name="token_btn_Back" value="TAddVinBack_AND" />
				<step type="CALL2" name="fn_VerifyPageOpened_AddVIN" page="pge_AddVIN"/>
			</step>			
			
			<step type="ELSE" name="elseCondition" page="pge_AddVIN">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
					<parameter type="STATIC" value="Find VIN page is not displayed or opened" />
				</step>
			</step>
		</step>
		
		<step type="CLICK" name="lnk_PrivacyPolicy" page="pge_AddVIN" tagNames="FP-EU" os="ANDROID">
			<token type="CONTENT" name="token_lnk_PrivacyPolicy" value="TPrivacyPolicy" />
		
			<step type="WAIT_FOR" name="lbl_PrivacyPolicyContent" page="pge_AddVIN"  failureMode="IGNORE">
				<parameter type="STATIC" value="15" />
				<token type="CONTENT" name="token_lbl_PrivacyPolicyContent" value="TPrivacyPolicyContent" />
			</step>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on PrivacyPolicy page" />
			</step>
			
			<step type="EXISTS" name="btn_BackInPrivacyPolicy" page="pge_AddVIN" />			
			
			<step type="CLICK" name="btn_BackInPrivacyPolicy" page="pge_AddVIN" >
				<step type="CALL2" name="fn_VerifyPageOpened_AddVIN" page="pge_AddVIN"/>
			</step>	
			
		<step type="ELSE" name="elseCondition" page="pge_AddVIN">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="PrivacyPolicy link is not displayed" />
			</step>
		</step>			
		</step>
	</function>
	
	<function name="fn_AddVehicle">
		<description> fn_AddVehicle - Function to add a new vehicle in dashboard </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>
		
		<step type="CALL2" name="fn_ClickAddVehicle" page="pge_AddVIN" failureMode="IGNORE"/>
		
		<step type="CALL2" name="fn_VerifyPageOpened_AddVIN" page="pge_AddVIN" />
		
         <step type="EXISTS" name="btn_Next_Disabled" page="pge_AddVIN" >
            <token type="CONTENT" name="token_btn_Next_Disabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_IOS" />
            
         </step>
		
		<step type="SET" name="edt_VIN" page="pge_AddVIN" >
			<parameter type="DATA" name="VIN id" value="TestData.VIN" />
		</step>
		
		<step type="CLICK" name="lbl_AddVehicleHeader" page="pge_AddVIN" >
			<token type="CONTENT" name="token_lbl_AddVehicleHeader" value="TAddVinHeader" />
		</step>
		
       	<step type="CLICK" name="btn_Next_Enabled" page="pge_AddVIN" >
        	<token type="CONTENT" name="token_btn_Next_Enabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_LowerCase" value="TCommon_KeyboardNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />                     
            <token type="CONTENT" name="token_btn_Next_Enabled_IOS" value="TSelectCountry2_IOS" />
        </step>
		
       	<!-- <step type="CLICK" name="btn_Next_Enabled" page="pge_AddVIN" >
        	<token type="CONTENT" name="token_btn_Next_Enabled" value="TAddVinNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_LowerCase" value="TCommon_KeyboardNext" />
            <token type="CONTENT" name="token_btn_Next_Disabled_IOS" value="TSelectCountry2_AND" />                     
        </step> -->
		
		<step type="WAIT_FOR" name="lbl_VehicleFound" page="pge_AddVIN" >
			<parameter type="STATIC" value="10" />
			<token type="CONTENT" name="token_lbl_VehicleFound" value="TAddVinFound" />
			<token type="CONTENT" name="token_lbl_VehicleFound_AND" value="TAddVinFound_AND" />
			
																					  
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="User successfully landed on Add VIN success page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_AddVIN">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
					<parameter type="STATIC" value="Add VIN success page is not displayed or opened" />
				</step>
			</step>			
		</step>
		
		<step type="EXISTS" name="lbl_VehicleMakeModel" page="pge_AddVIN" failureMode="IGNORE">
			<token type="DATA" name="token_lbl_VehicleMakeModel" value="TestData.VinMakeModel" />
		</step>
		
	</function>
	
	<function name="fn_AddVehicle_SetNickname">
		<description> fn_AddVehicle_SetNickname - Function to set nickname in add vehicle screen </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>	
		
		<step type="SET" name="edt_Nickname" page="pge_AddVIN" >
			<parameter type="CONTEXT" name="vehicle nickname" value="param_Nickname" />
			<token type="CONTENT" name="token_edt_Nickname" value="TAddVinNickName" />
		</step>
	
	</function>
	
  	<function name="fn_ClickBtnGetStarted">
		<description> fn_ClickBtnGetStarted - Function to click button - GetStarted in add vehicle screen </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
			<parameter type="STATIC" value="Function's XML file name: fn_AddVIN.xml" />
		</step>	
		<step type="CLICK" name="btn_Return_kboard" page="pge_AddVIN" os="IOS">
			<token type="CONTENT" name="token_btn_Return_kboard" value="TReturnKey_IOS"/>
		</step>
		
		<step type="CALL2" name="fn_HideKeyboard" page="pge_CommonReusable" failureMode="IGNORE"/>
		
		<step type="VISIBLE" name="btn_GetStarted" page="pge_AddVIN" >
			<parameter type="STATIC" name="maxScroll" value="3" />
			<parameter type="STATIC" name="direction" value="UP" />
			<token type="CONTENT" name="token_btn_GetStarted" value="TAddVinGetStarted" />
			
		<step type="CLICK" name="btn_GetStarted" page="pge_AddVIN" >
			<token type="CONTENT" name="token_btn_GetStarted" value="TAddVinGetStarted" />
		</step>	
		
		<step type="WAIT" name="Wait for 5 sec" page="pge_AddVIN">
		<parameter type="STATIC" value="5"/>
		
		</step>
		</step>
	
	</function>
	
	
