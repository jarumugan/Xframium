	<function name="fn_VerifyPageOpened_EditProfile">
		<description> fn_VerifyPageOpened_EditProfile - Function to verify user landed on the 'Edit Profile' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
			<parameter type="STATIC" value="Function's XML file name: fn_EditProfile.xml" />
		</step>
		
		<step type="WAIT_FOR" name="lbl_PageHeader" page="pge_EditProfile">
			<parameter type="STATIC" value="30" />
			<token type="CONTENT" name="token_lbl_PageHeader" value="TEditProfile1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
				<parameter type="STATIC" value="User successfully landed on 'Edit Profile' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EditProfile">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="STATIC" value="'Edit Profile' page is not displayed or opened" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_ClickEditProfile_Cancel">
		<description> fn_ClickEditProfile_Cancel - Function to click CANCEL link on 'Edit Profile' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
			<parameter type="STATIC" value="Function's XML file name: fn_EditProfile.xml" />
		</step>
		
		<step type="EXISTS" name="lnk_Cancel" page="pge_EditProfile">
			<token type="CONTENT" name="token_lnk_Cancel" value="TEditProfile16" />
			
			<step type="CLICK" name="lnk_Cancel" page="pge_EditProfile" wait="3">
				<token type="CONTENT" name="token_lnk_Cancel" value="TEditProfile16" />
			</step>
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
				<parameter type="STATIC" value="'Cancel' link is available and clicked on 'Edit Profile' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EditProfile">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EditProfile">
					<parameter type="STATIC" value="'Cancel' link is not available on 'Edit Profile' page" />
				</step>
			</step>
		</step>
	</function>
	
