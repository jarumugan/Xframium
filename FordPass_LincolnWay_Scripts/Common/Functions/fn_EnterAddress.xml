		</step>
		
		<step type="CALL2" name="fn_ValidateEnterAddress_NextButtonStatus" page="pge_EnterAddress">
			<parameter type="STATIC" name="param_ExpectedStatus" value="false" />
		</step>
		
		<step type="EXISTS" name="lbl_PhoneNumber" page="pge_EnterAddress" tagNames="FP-EU" os="ANDROID">
			<token type="CONTENT" name="token_lbl_PhoneNumber" value="TEnterAddress16" />
						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="STATIC" value="'Phone Number' section header displayed as expected on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Phone Number' section header is not displayed on 'Enter Address' page" />
				</step>
			</step>
		</step>
		
		<step type="EXISTS" name="lbl_Note" page="pge_EnterAddress">
			<token type="CONTENT" name="token_lbl_Note" value="TEnterAddress10" />
						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="STATIC" value="'Note' section header displayed as expected on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Note' section header is not displayed on 'Enter Address' page" />
				</step>
			</step>
		</step>
		
		<step type="EXISTS" name="lbl_NoteDesc" page="pge_EnterAddress">
			<token type="CONTENT" name="token_lbl_NoteDesc" value="TEnterAddress11" />
						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTENT" value="TEnterAddress11" />
				<parameter type="STATIC" value="Above description text displayed on Note section as expected on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTENT" value="TEnterAddress11" />
					<parameter type="STATIC" value="Above description text is not displayed on Note section on 'Enter Address' page" />
				</step>
