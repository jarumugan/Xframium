<!-- EMUZZAMM - This function is to enable Access FordPass Smartlink checkbox									   -->	
<!-- ############################################################################################################# -->	    
     
     <function name="fn_Enable_Smartlink_Checkbox">
              <description> fn_Enable_Smartlink_Checkbox - Function to enable Access FordPass Smartlink checkbox</description>
              <step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
                     <parameter type="STATIC" value="Function's XML file name: pge_AddVIN" />
              </step>
     
     
	<step type="CLICK" name="btn_AccessFordPass_Smartlink_CheckBox" page="pge_AddVIN">
	
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="'ACCESS FORDPASS SMARTLINK' Info screen is displayed" />
		</step>
		
		<step type="ELSE" name="elseCondition" page="pge_AddVIN">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
				<parameter type="STATIC" value="'ACCESS FORDPASS SMARTLINK' Info screen is not displayed" />
			</step>
		</step>		
	</step>
	
	<step type="WAIT" name="Allowing time to check" page="pge_AddVIN">
	<parameter type="STATIC" value="5"/>
	</step>
	
	<step type="EXISTS" name="btn_AccessFordPass_Smartlink_CheckBox_Checked" page="pge_AddVIN">
	   			<step type="REPORT" name="ADD_TO_REPORT" page="pge_AddVIN">
	<parameter type="STATIC" value="'ACCESS FORDPASS SMARTLINK' checkbox is checked" />
	</step>
	
	</step>
	
</function>

		
	
</suite>