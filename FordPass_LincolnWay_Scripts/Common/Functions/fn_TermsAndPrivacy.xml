	<function name="fn_VerifyPageOpened_TermsandPrivacy">
	<description> fn_VerifyPageOpened_TermsandPrivacy - Verify 'Privacy Policy' page is loaded  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy" >
			<parameter type="STATIC" value="*** XML FILE NAME OF THE CALLED FUNCTION: fn_TermsAndPrivacy.xml ***" />
		</step>

		<step type="WAIT_FOR" name="lbl_PageTitle" site="FordPass" page="pge_TermsAndPrivacy">
			<parameter type="STATIC" name="Wait Time" value="60"/>
			<token type="CONTENT" name="token_lbl_TermsAndPrivacy" value="TTermsAndPrivacy1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy">
				<parameter type="STATIC" value="'Privacy Policy' page is opened and page title is as expected" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_TermsAndPrivacy">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy">
					<parameter type="STATIC" value="'Privacy Policy' page is not opened or page title is not as expected" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_ClickAcceptBtn">
		<description> Click the 'Jump To Login' link </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy" >
			<parameter type="STATIC" value="*** XML FILE NAME OF THE CALLED FUNCTION: fn_TermsAndPrivacy.xml ***" />
		</step>	
		<step name="btn_Accept.VISIBLE" site="FordPass" page="pge_TermsAndPrivacy" type="WAIT_FOR" failureMode="IGNORE">
			<parameter type="STATIC" name="Wait Time" value="15"/>
			<token type="CONTENT" name="token_btn_Accept" value="TTermsAndPrivacy2" />
		</step>		
		<step name="btn_Accept" site="FordPass" page="pge_TermsAndPrivacy" type="CLICK">
				<token type="CONTENT" name="token_btn_Accept" value="TTermsAndPrivacy2" />
		</step>
	</function>
	
	<function name="fn_ClickBackIcon">
		<description> Click on the Back Icon</description>
		<step type="CLICK" name="img_Back" page="pge_TermsAndPrivacy" />
	</function>
	
	
	<function name="Fn_VerifyTermsPrivacyUpdate">
	
		<description> Verify Terms and Condition update is getting displayed</description>
		<step type="EXISTS" name="lbl_TermsPrivacyUpdate" page="pge_TermsAndPrivacy">
			<token type="CONTENT" name="token_lbl_TermsPrivacyUpdate" value="TTermsandPrivacyUpdate1" />
			<step type="ELSE" name="Else Condition" page="pge_TermsAndPrivacy">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy" >
					<parameter type="STATIC" value="Terms and Privacy Update message is not getting displayed." />
				</step>	
			</step>
		</step>
		
		<step type="EXISTS" name="lbl_TermsPrivacyMessage" page="pge_TermsAndPrivacy">
			<token type="CONTENT" name="token_lbl_TermsPrivacyMessage" value="TTermsandPrivacyUpdate2" />
			<step type="ELSE" name="Else Condition" page="pge_TermsAndPrivacy">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_TermsAndPrivacy" >
					<parameter type="STATIC" value="Terms and Privacy Update message is not getting displayed." />
				</step>	
			</step>
		</step>
		
	</function>
	
	<function name="fn_ClickViewTermsPrivacy">
		
		<step type="CLICK" name="btn_ViewTermsandConditions" page="pge_TermsAndPrivacy">
