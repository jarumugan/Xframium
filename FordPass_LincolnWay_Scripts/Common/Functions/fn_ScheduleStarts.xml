     <function name="fn_Click_NextScheduleStart">
              <description> fn_Click_NextScheduleStart - Function to click on next schedule start</description>
              <step type="REPORT" name="ADD_TO_REPORT" page="pge_ScheduleStarts">
                     <parameter type="STATIC" value="Function's XML file name: fn_ScheduleStarts.xml" />
              </step>
              
      
     <step type="VISIBLE" name="lnk_NextScheduleStart_Title" page="pge_ScheduleStarts">
     	   	<parameter type="STATIC" name="maxScroll"  value="5"/>
			<parameter type="STATIC" name="direction" value="UP"/>
     	<token type="CONTENT" name="token_lnk_NextScheduleStart_Title" value="TScheduleStart1"/>
     </step>        
      
     
     <step type="CLICK" name="lnk_NextScheduleStart_Title" page="pge_ScheduleStarts">
     	<token type="CONTENT" name="token_lnk_NextScheduleStart_Title" value="TScheduleStart1"/>
     
     		<step type="WAIT_FOR" name="btn_AddStart" page="pge_ScheduleStarts">
     		<parameter type="STATIC"  value="30"/>
     		<token type="CONTENT" name="token_btn_AddStart" value="TScheduleStart3"/>
     		</step>
		
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_ScheduleStarts">
				<parameter type="STATIC" value="Clicked on Next Schedule Start" />
		</step>
		
		<step type="ELSE" name="elseCondition" page="pge_ScheduleStarts">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_ScheduleStarts">
				<parameter type="STATIC" value="Not clicked on Next Schedule Start"  />
			</step>
		</step>			
		
	</step>
 
 	</function>
 	
 	
