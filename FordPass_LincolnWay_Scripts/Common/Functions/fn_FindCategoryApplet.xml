	<!-- ESURESH8 - This functions to verify and validate the 'CATEGORY TRAY BAR' existence by passing it as a Parameter -->
	<!-- ESURESH8 - Required Call Function with a Parameter to execute this function  -->
	<!-- ############################################################################################################# -->
		
	<function name="fn_Verify_CategoryTrayBar">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Category Tray Bar' Presence  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>		
		<step type="EXISTS" name="btn_CategoryTrayBar" page="pge_FindCategoryApplet">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY BAR' is EXISTS on the Page" />
			</step>
		<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY BAR' is NOT EXISTS on the Page" />
			</step>
		</step>
		</step>
	</function>
	
	<function name="fn_Click_CategoryTrayBar">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Category Tray Bar' Presence  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>		
		<step type="CLICK" name="btn_CategoryTrayBar" page="pge_FindCategoryApplet">
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY BAR' is CLICKED and displayed on the Page" />
			</step>
		<step type="ELSE" name="elseCondition" page="pge_FindCategoryApplet">						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet">
				<parameter type="STATIC" value="'CATEGORY TRAY BAR' NOT CLICKED on the Page" />
			</step>
		</step>
		</step>
	</function>
	
	<function name="fn_Validate_Categories">
	<description> fn_Verify_CategoryTrayBar: To verify the 'Categories' on Tray  </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_FindCategoryApplet" >
			<parameter type="STATIC" value="Function's XML file name: fn_FindCategoryApplet.xml" />
		</step>	
		<!-- Verify Categories Tray 1 Categories Presence-->
		<step type="CALL2" name="fn_Verify_Categories" page="pge_FindCategoryApplet" >
			<parameter type="CONTENT" name="param_Categories_IOS" value="TFindCategory1_IOS" />
			<parameter type="CONTENT" name="param_Categories_AND" value="TFindCategory1_AND" />
		</step>
		<step type="CALL2" name="fn_Verify_Categories" page="pge_FindCategoryApplet" >
			<parameter type="CONTENT" name="param_Categories_IOS" value="TFindCategory2_IOS" />
			<parameter type="CONTENT" name="param_Categories_AND" value="TFindCategory2_AND" />
		</step>
		<step type="CALL2" name="fn_Verify_Categories" page="pge_FindCategoryApplet" >
			<parameter type="CONTENT" name="param_Categories_IOS" value="TFindCategory3_IOS" />
