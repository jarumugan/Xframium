	<function name="fn_VerifyPageOpened_EnterAddress">
		<description> fn_VerifyPageOpened_EnterAddress - Function to verify user landed on the 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="VISIBLE" name="lbl_PageTitle" page="pge_EnterAddress" wait="3">
			<parameter type="STATIC" name="maxScroll" value="2" />
			<parameter type="STATIC" name="direction" value="DOWN" />
			<token type="CONTENT" name="token_lbl_PageTitle" value="TEnterAddress1" />
			
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="STATIC" value="User successfully landed on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Enter Address' page is not displayed or page header is not as expected" />
				</step>
			</step>
		</step>
	</function>
	
	<function name="fn_ValidatePage_EnterAddress">
		<description> fn_ValidatePage_EnterAddress - Function to validate 'Enter Address' page </description>
		<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
			<parameter type="STATIC" value="Function's XML file name: fn_EnterAddress.xml" />
		</step>
		
		<step type="EXISTS" name="lbl_PageInfo" page="pge_EnterAddress">
			<token type="CONTENT" name="token_lbl_PageInfo_AND" value="TEnterAddress2" />
			<token type="CONTENT" name="token_lbl_PageInfo_IOS" value="TEnterAddress2_IOS" />
						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="CONTENT" value="TEnterAddress2" />
				<parameter type="STATIC" value="Above text is displayed as page information on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="CONTENT" value="TEnterAddress2" />
					<parameter type="STATIC" value="Above text is not displayed as page information on 'Enter Address' page" />
				</step>
			</step>
		</step>

		<step type="EXISTS" name="lbl_PrimaryAddress" page="pge_EnterAddress" os="ANDROID">
			<token type="CONTENT" name="token_lbl_PrimaryAddress" value="TEnterAddress3" />
						
			<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
				<parameter type="STATIC" value="'Primary Address' section header displayed as expected on 'Enter Address' page" />
			</step>
			
			<step type="ELSE" name="elseCondition" page="pge_EnterAddress">
				<step type="REPORT" name="ADD_TO_REPORT" page="pge_EnterAddress">
					<parameter type="STATIC" value="'Primary Address' section header is not displayed on 'Enter Address' page" />
